{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/build/"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/build"
  ]
}